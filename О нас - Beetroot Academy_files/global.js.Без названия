c){var d,e,f,g,j,h=null,i=this;return b=b||!1,i.animating===!0&&i.options.waitForAnimate===!0||i.options.fade===!0&&i.currentSlide===a||i.slideCount<=i.options.slidesToShow?void 0:(b===!1&&i.asNavFor(a),d=a,h=i.getLeft(d),g=i.getLeft(i.currentSlide),i.currentLeft=null===i.swipeLeft?g:i.swipeLeft,i.options.infinite===!1&&i.options.centerMode===!1&&(0>a||a>i.getDotCount()*i.options.slidesToScroll)?void(i.options.fade===!1&&(d=i.currentSlide,c!==!0?i.animateSlide(g,function(){i.postSlide(d)}):i.postSlide(d))):i.options.infinite===!1&&i.options.centerMode===!0&&(0>a||a>i.slideCount-i.options.slidesToScroll)?void(i.options.fade===!1&&(d=i.currentSlide,c!==!0?i.animateSlide(g,function(){i.postSlide(d)}):i.postSlide(d))):(i.options.autoplay&&clearInterval(i.autoPlayTimer),e=0>d?i.slideCount%i.options.slidesToScroll!==0?i.slideCount-i.slideCount%i.options.slidesToScroll:i.slideCount+d:d>=i.slideCount?i.slideCount%i.options.slidesToScroll!==0?0:d-i.slideCount:d,i.animating=!0,i.$slider.trigger("beforeChange",[i,i.currentSlide,e]),f=i.currentSlide,i.currentSlide=e,i.setSlideClasses(i.currentSlide),i.options.asNavFor&&(j=i.getNavTarget(),j=j.slick("getSlick"),j.slideCount<=j.options.slidesToShow&&j.setSlideClasses(i.currentSlide)),i.updateDots(),i.updateArrows(),i.options.fade===!0?(c!==!0?(i.fadeSlideOut(f),i.fadeSlide(e,function(){i.postSlide(e)})):i.postSlide(e),void i.animateHeight()):void(c!==!0?i.animateSlide(h,function(){i.postSlide(e)}):i.postSlide(e))))},b.prototype.startLoad=function(){var a=this;a.options.arrows===!0&&a.slideCount>a.options.slidesToShow&&(a.$prevArrow.hide(),a.$nextArrow.hide()),a.options.dots===!0&&a.slideCount>a.options.slidesToShow&&a.$dots.hide(),a.$slider.addClass("slick-loading")},b.prototype.swipeDirection=function(){var a,b,c,d,e=this;return a=e.touchObject.startX-e.touchObject.curX,b=e.touchObject.startY-e.touchObject.curY,c=Math.atan2(b,a),d=Math.round(180*c/Math.PI),0>d&&(d=360-Math.abs(d)),45>=d&&d>=0?e.options.rtl===!1?"left":"right":360>=d&&d>=315?e.options.rtl===!1?"left":"right":d>=135&&225>=d?e.options.rtl===!1?"right":"left":e.options.verticalSwiping===!0?d>=35&&135>=d?"down":"up":"vertical"},b.prototype.swipeEnd=function(a){var c,d,b=this;if(b.dragging=!1,b.interrupted=!1,b.shouldClick=b.touchObject.swipeLength>10?!1:!0,void 0===b.touchObject.curX)return!1;if(b.touchObject.edgeHit===!0&&b.$slider.trigger("edge",[b,b.swipeDirection()]),b.touchObject.swipeLength>=b.touchObject.minSwipe){switch(d=b.swipeDirection()){case"left":case"down":c=b.options.swipeToSlide?b.checkNavigable(b.currentSlide+b.getSlideCount()):b.currentSlide+b.getSlideCount(),b.currentDirection=0;break;case"right":case"up":c=b.options.swipeToSlide?b.checkNavigable(b.currentSlide-b.getSlideCount()):b.currentSlide-b.getSlideCount(),b.currentDirection=1}"vertical"!=d&&(b.slideHandler(c),b.touchObject={},b.$slider.trigger("swipe",[b,d]))}else b.touchObject.startX!==b.touchObject.curX&&(b.slideHandler(b.currentSlide),b.touchObject={})},b.prototype.swipeHandler=function(a){var b=this;if(!(b.options.swipe===!1||"ontouchend"in document&&b.options.swipe===!1||b.options.draggable===!1&&-1!==a.type.indexOf("mouse")))switch(b.touchObject.fingerCount=a.originalEvent&&void 0!==a.originalEvent.touches?a.originalEvent.touches.length:1,b.touchObject.minSwipe=b.listWidth/b.options.touchThreshold,b.options.verticalSwiping===!0&&(b.touchObject.minSwipe=b.listHeight/b.options.touchThreshold),a.data.action){case"start":b.swipeStart(a);break;case"move":b.swipeMove(a);break;case"end":b.swipeEnd(a)}},b.prototype.swipeMove=function(a){var d,e,f,g,h,b=this;return h=void 0!==a.originalEvent?a.originalEvent.touches:null,!b.dragging||h&&1!==h.length?!1:(d=b.getLeft(b.currentSlide),b.touchObject.curX=void 0!==h?h[0].pageX:a.clientX,b.touchObject.curY=void 0!==h?h[0].pageY:a.clientY,b.touchObject.swipeLength=Math.round(Math.sqrt(Math.pow(b.touchObject.curX-b.touchObject.startX,2))),b.options.verticalSwiping===!0&&(b.touchObject.swipeLength=Math.round(Math.sqrt(Math.pow(b.touchObject.curY-b.touchObject.startY,2)))),e=b.swipeDirection(),"vertical"!==e?(void 0!==a.originalEvent&&b.touchObject.swipeLength>4&&a.preventDefault(),g=(b.options.rtl===!1?1:-1)*(b.touchObject.curX>b.touchObject.startX?1:-1),b.options.verticalSwiping===!0&&(g=b.touchObject.curY>b.touchObject.startY?1:-1),f=b.touchObject.swipeLength,b.touchObject.edgeHit=!1,b.options.infinite===!1&&(0===b.currentSlide&&"right"===e||b.currentSlide>=b.getDotCount()&&"left"===e)&&(f=b.touchObject.swipeLength*b.options.edgeFriction,b.touchObject.edgeHit=!0),b.options.vertical===!1?b.swipeLeft=d+f*g:b.swipeLeft=d+f*(b.$list.height()/b.listWidth)*g,b.options.verticalSwiping===!0&&(b.swipeLeft=d+f*g),b.options.fade===!0||b.options.touchMove===!1?!1:b.animating===!0?(b.swipeLeft=null,!1):void b.setCSS(b.swipeLeft)):void 0)},b.prototype.swipeStart=function(a){var c,b=this;return b.interrupted=!0,1!==b.touchObject.fingerCount||b.slideCount<=b.options.slidesToShow?(b.touchObject={},!1):(void 0!==a.originalEvent&&void 0!==a.originalEvent.touches&&(c=a.originalEvent.touches[0]),b.touchObject.startX=b.touchObject.curX=void 0!==c?c.pageX:a.clientX,b.touchObject.startY=b.touchObject.curY=void 0!==c?c.pageY:a.clientY,void(b.dragging=!0))},b.prototype.unfilterSlides=b.prototype.slickUnfilter=function(){var a=this;null!==a.$slidesCache&&(a.unload(),a.$slideTrack.children(this.options.slide).detach(),a.$slidesCache.appendTo(a.$slideTrack),a.reinit())},b.prototype.unload=function(){var b=this;a(".slick-cloned",b.$slider).remove(),b.$dots&&b.$dots.remove(),b.$prevArrow&&b.htmlExpr.test(b.options.prevArrow)&&b.$prevArrow.remove(),b.$nextArrow&&b.htmlExpr.test(b.options.nextArrow)&&b.$nextArrow.remove(),b.$slides.removeClass("slick-slide slick-active slick-visible slick-current").attr("aria-hidden","true").css("width","")},b.prototype.unslick=function(a){var b=this;b.$slider.trigger("unslick",[b,a]),b.destroy()},b.prototype.updateArrows=function(){var b,a=this;b=Math.floor(a.options.slidesToShow/2),a.options.arrows===!0&&a.slideCount>a.options.slidesToShow&&!a.options.infinite&&(a.$prevArrow.removeClass("slick-disabled").attr("aria-disabled","false"),a.$nextArrow.removeClass("slick-disabled").attr("aria-disabled","false"),0===a.currentSlide?(a.$prevArrow.addClass("slick-disabled").attr("aria-disabled","true"),a.$nextArrow.removeClass("slick-disabled").attr("aria-disabled","false")):a.currentSlide>=a.slideCount-a.options.slidesToShow&&a.options.centerMode===!1?(a.$nextArrow.addClass("slick-disabled").attr("aria-disabled","true"),a.$prevArrow.removeClass("slick-disabled").attr("aria-disabled","false")):a.currentSlide>=a.slideCount-1&&a.options.centerMode===!0&&(a.$nextArrow.addClass("slick-disabled").attr("aria-disabled","true"),a.$prevArrow.removeClass("slick-disabled").attr("aria-disabled","false")))},b.prototype.updateDots=function(){var a=this;null!==a.$dots&&(a.$dots.find("li").removeClass("slick-active").attr("aria-hidden","true"),a.$dots.find("li").eq(Math.floor(a.currentSlide/a.options.slidesToScroll)).addClass("slick-active").attr("aria-hidden","false"))},b.prototype.visibility=function(){var a=this;a.options.autoplay&&(document[a.hidden]?a.interrupted=!0:a.interrupted=!1)},a.fn.slick=function(){var f,g,a=this,c=arguments[0],d=Array.prototype.slice.call(arguments,1),e=a.length;for(f=0;e>f;f++)if("object"==typeof c||"undefined"==typeof c?a[f].slick=new b(a[f],c):g=a[f].slick[c].apply(a[f].slick,d),"undefined"!=typeof g)return g;return a}});
;/* ========================================================================
 * # Main JS file
 *    Docs:
 *    http://api.jquery.com/
 *    https://github.com/AllThingsSmitty/jquery-tips-everyone-should-know
 *    https://github.com/peterkokot/awesome-jquery
 ========================================================================*/
function width_svg_polygon(){
        var windowWidth = $(window).width();
        $('.background_video svg').attr({
              width: windowWidth,
            });
        var polygon = '0,70 '+windowWidth+',0 '+windowWidth+',70';
        $('.background_video polygon').attr({
              points: polygon,
            });
        ///////////////////////////////////////////////////
        var parentHeight = $('.thumbnail_course').height();
        $('.thumbnail_course svg').attr({
              height: parentHeight,
            });
        var polygon = '0,0 12,0 12,'+parentHeight+'';
        $('.thumbnail_course polygon').attr({
              points: polygon,
            });
        /////////////////////////////////////////
        // $('.courses_general .skew svg').attr({
        //       width: windowWidth,
        //     });
        // var polygon = '0,0 '+windowWidth+',25 0,25';
        // $('.courses_general .skew svg polygon').attr({
        //       points: polygon,
        //     });
        ////////////////////////////////////////
        var parentWidth = $('.person').width();
        $('.person .info svg').attr({
              width: parentWidth,
            });
        var polygon = '0,0 '+parentWidth+',10 0,10';
        $('.person .info svg polygon').attr({
              points: polygon,
            });
        var parentWidth = $('.thumbnail_team').width();
        $('.person .thumbnail_team svg').attr({
              width: parentWidth,
            });
        var polygon = '0,0 '+parentWidth+',10 0,10';
        $('.person .thumbnail_team svg polygon').attr({
              points: polygon,
            });
        ////////////////////////////////////////
         var parentWidth = $('.locations .towns .town .skew').width();
        $('.locations .towns .town .skew svg').attr({
              width: parentWidth,
            });
        var polygon = '0,0 '+parentWidth+',10 0,10';
        $('.locations .towns .town .skew svg polygon').attr({
              points: polygon,
            });
        ////////////////////////////////////////////////////////
        // $('.location .skew .head svg').attr({
        //       width: windowWidth,
        //     });
        // var polygon = '0,25 '+windowWidth+',0 '+windowWidth+',25';
        // $('.location .skew .head svg polygon').attr({
        //       points: polygon,
        //     });
        ////////////////////////////////////////////////////////
        var parentWidth = $('.place .skew').width();
        $('.place .address svg').attr({
              width: parentWidth,
            });
        var polygon = '0,0 '+parentWidth+',20 0,20';
        $('.place .address svg polygon').attr({
              points: polygon,
            });
        ////////////////////////////////////////////////////////
        $('#map svg').attr({
              width: windowWidth,
            });
        var polygon = '0,25 '+windowWidth+',0 '+windowWidth+',25';
        $('#map svg polygon').attr({
              points: polygon,
            });
        ////////////////////////////////////////////////////////
        var parentWidth = $('.event_sidebar .thumbnail_event').width();
        $('.event_sidebar .thumbnail_event svg').attr({
              width: parentWidth,
            });
        var polygon = '0,0 '+parentWidth+',15 0,15';
        $('.event_sidebar .thumbnail_event svg polygon').attr({
              points: polygon,
            });
        ////////////////////////////////////////////////////////////
         $('.about .head svg').attr({
              width: windowWidth,
            });
        var polygon = '0,25 '+windowWidth+',0 '+windowWidth+',25';
        $('.about .head svg polygon').attr({
              points: polygon,
            });
}
(function ($) {
    // Scripts that will only run once the page Document Object Model (DOM) is ready for JavaScript code to execute.
    //Uncomment this if you are using Foundation
    $(document).ready(function () {
        width_svg_polygon();
    });

    // Scripts that will run after the whole page is loaded (images, videos, iframes. etc)
    $(window).on('load', function () {
      width_svg_polygon();
    });

    // Scripts that will run on window resize
    $(window).on('resize', function () {
        width_svg_polygon();
    });

})(jQuery); // Fully reference jQuery after this point.

;/* ========================================================================
 * DOM-based Routing
 * Based on http://goo.gl/EUTi53 by Paul Irish
 *
 * Only fires on body classes that match. If a body class contains a dash,
 * replace the dash with an underscore when adding it to the object below.
 *
 * .noConflict()
 * The routing is enclosed within an anonymous function so that you can
 * always reference jQuery with $, even when in .noConflict() mode.
 * ======================================================================== */

(function ($) {
    // Use this variable to set up the common and page specific functions. If you
    // rename this variable, you will also need to rename the namespace below.
    var animateTime = 300; // time for height animation
    var $body = $('body');
    var Sage = {
        // All pages
        'common': {
            init: function () {
                // JavaScript to be fired on all pages

                // Add IE class
                if (navigator.userAgent.match(/Trident\/7\./)) {
                    $body.addClass('ie');
                }
                if (/Edge\/\d./i.test(navigator.userAgent)) {
                    // This is Microsoft Edge
                    $body.addClass('ie');
                }

                // Set footer height for parallax scroll
                var heightFooter = $('#footer-container').outerHeight();
                $('#content').css('margin-bottom', heightFooter + 'px');

                // MatchHeight for elements
                $('.js-matchheight').matchHeight({
                    byRow: false
                });
                $('.js-matchheight-byrow').matchHeight({
                    byRow: true
                });
                // In case other element on page have same class and conflicts
                $('.js-matchheight-custom').matchHeight({
                    byRow: false
                });
                // Init Select2 lib
                $('.select2').select2({
                    tags: true,
                    minimumResultsForSearch: -1,
                    dropdownAutoWidth: true,
                    width: 'auto',
                    sorter: function (results) {
                        var query = $('.select2-search__field').val().toLowerCase();
                        return results.sort(function (a, b) {
                            return a.text.toLowerCase().indexOf(query) -
                                b.text.toLowerCase().indexOf(query);
                        });
                    },
                    matcher: function (params, data) {
                        // If there are no search terms, return all of the data
                        if ($.trim(params.term) === '') {
                            return data;
                        }

                        // Do not display the item if there is no 'text' property
                        if (typeof data.text === 'undefined') {
                            return null;
                        }

                        // `params.term` should be the term that is used for searching
                        // `data.text` is the text that is displayed for the data object
                        if (data.text.indexOf(params.term) > -1) {
                            var modifiedData = $.extend({}, data, true);

                            // You can return modified objects from here
                            // This includes matching the `children` how you want in nested data sets
                            return modifiedData;
                        }

                        //Disable case sensitive search field
                        if (data.text.toUpperCase().indexOf(params.term.toUpperCase()) > -1) {
                            var modifiedData = $.extend({}, data, true);
                            return modifiedData;
                        }

                        // Return `null` if the term should not be displayed
                        return null;
                    }
                });

                // Fix select2 search field
                $.fn.modal.Constructor.prototype.enforceFocus = function () {
                };


                // Mobile menu toggles
                $('#mobile_menu .dropdown-toggle').on("click", function (e) {
                    $(this).next('ul').toggle();
                    e.stopPropagation();
                });
                $('.burger_menu a').on('click', function (e) {
                    e.preventDefault();
                    $(this).parent().toggleClass('active');
                    $(this).toggleClass('active');
                    $(this).parent().siblings('nav.mobile_nav').toggleClass('active');
                });
                $('.multi_language').on('mouseover', function () {
                    $('.dropdown.open').removeClass('open');
                    $(this).addClass('open');
                });
                $('.multi_language .dropdown-menu').on('mouseout', function () {
                    $('.multi_language').removeClass('open');
                });
                $('#menu-item-courses').on('mouseover', function () {
                    $('.dropdown.open').removeClass('open');
                    $(this).addClass('open');
                });
                $('.locations-dropdown').on('mouseout', function () {
                    $('.dropdown.open').removeClass('open');
                });
                $('.courses-dropdown').on('mouseout', function () {
                    $('.dropdown.open').removeClass('open');
                });
                $('#menu-item-locations').on('mouseover', function () {
                    $('.dropdown.open').removeClass('open');
                    $(this).addClass('open');
                });
                $body.on('click', function (e) {
                    if ($(e.target).parents('dropdown').length === 0) {
                        $('.dropdown.open').removeClass('open');
                    }
                });
                // Label effects for all pages
                var formInputs = $('input, #country');
                formInputs.focus(function () {
                    $(this).parents('.form-item-wrap').addClass('active');
                });
                formInputs.blur(function () {
                    if ($(this).val() !== "") {
                        $(this).parents('.form-item-wrap').addClass('active');
                    }
                    else {
                        $(this).parents('.form-item-wrap').removeClass('active');
                    }
                });

                $(document).on('input', '#phone_modal', function () {
                    var val = $(this).val().trim(),
                        limitNumber = 9;
                    if (!val.match(/^\d+$/)) {
                        $(this).val('');
                    }
                });

                $('.form_subscribe .email input, .sidebar_story .email input').focus(function () {
                    $(this).parent('span').parent('.email').children('label').css('top', '40' + 'px');
                    $(this).parent('span').parent('.email').children('span.border').addClass('active');
                });
                $('.form_subscribe .email input, .sidebar_story .email input').blur(function () {
                    if ($(this).val() !== "") {
                        $(this).parent('span').parent('.email').children('label').css('top', '40' + 'px');
                        $(this).parent('span').parent('.email').children('span.border').addClass('active');
                    }
                    else {
                        $(this).parent('span').parent('.email').children('label').css('top', '0');
                        $(this).parent('span').parent('.email').children('span').removeClass('active');
                    }
                });

                $('#mail_subscribe').focus(function () {
                    $(this).closest('.email').children('label').css({
                        'top': '0' + 'px',
                        'opacity': '0.6',
                        'transform': 'scale(0.8) translate3d(0, -26px, 0)'
                    });
                    $(this).closest('.email').children('span.border').addClass('active');
                });
                $('#mail_subscribe').blur(function () {
                    if ($(this).val() !== "") {
                        $(this).closest('.email').children('label').css({
                            'top': '0' + 'px',
                            'opacity': '0.6',
                            'transform': 'scale(0.8) translate3d(0, -26px, 0)'
                        });
                        $(this).closest('.email').children('span.border').addClass('active');
                    }
                    else {
                        $(this).closest('.email').children('label').css('top', '0');
                        $(this).closest('.email').children('label').css({
                            'top': '0',
                            'opacity': '1',
                            'transform': 'none'
                        });
                        $(this).closest('.email').children('span').removeClass('active');
                    }
                });

                // Slidetoggle for towns dropdown
                $(document).on('click', '.towns__toggle', function (e) {
                    e.preventDefault();
                    e.stopPropagation();
                    var $cityList = $(this).next('ul');
                    if ($cityList.height() === 0) {
                        autoHeightAnimate($cityList, animateTime);
                    } else {
                        $cityList.stop().animate({height: '0'}, animateTime);
                    }
                });
                $('ul.towns').click(function (e) {
                    var no_active_towns = $(this).find('.no_active');
                    if ($('li', no_active_towns).length > 0) {
                        $(this).toggleClass('active');
                        no_active_towns.toggleClass('active');
                    }
                });

                $(document).mouseup(function (e) {
                    var div = $("ul.towns");
                    if (!div.is(e.target)
                        && div.has(e.target).length === 0) {
                        $(this).find('.no_active').removeClass('active');
                        $('ul.towns').removeClass('active');
                    }
                });

                $('h4.somelocation ul.towns li ul.no_active li a, header ul.dropdown-menu li a').click(function (e) {
                    e.stopPropagation();
                });

                $('#ask_us').parent('span').css({
                    'width': '100' + '%',
                });
                $('select#course_location_modal').parent('p').children('label').css({
                    'top': '9' + 'px',
                    'font-size': '10' + 'px',
                    'color': 'rgba(61,57,53,0.5)'
                });

                //CSS for about academy page
                var thumbnailAbout = $('.about_academy .articles .article .thumbnail_article');
                var widthThumbnail = thumbnailAbout.width();
                thumbnailAbout.height(widthThumbnail);


                // Scripts that will run on window resize
                $(window).on('resize', function () {
                    var heightFooter = $('footer').height();
                    $('#content').css('margin-bottom', heightFooter + 'px');
                    $('.courses_archive .courses .course .row .col-md-6').matchHeight();

                    //CSS for about academy page
                    var thumb = $('.about_academy .articles .article .thumbnail_article');
                    thumb.height(thumb.width());
                });
            },
            finalize: function () {
                // JavaScript to be fired on all pages, after page specific JS is fired
            }
        },
        // Home page
        'home': {
            init: function () {
                // JavaScript to be fired on the home page
                $('.home-stories').slick({
                    infinite: false,
                    slidesToShow: 3,
                    slidesToScroll: 1,
                    prevArrow: $('.prev_story'),
                    nextArrow: $('.next_story'),
                    responsive: [
                        {
                            breakpoint: 769,
                            settings: {
                                slidesToShow: 2
                            }
                        },
                        {
                            breakpoint: 600,
                            settings: {
                                slidesToShow: 1
                            }
                        }
                    ]
                });
            },
            finalize: function () {
                // JavaScript to be fired on the home page, after the init JS
            }
        },
        // Course single page
        'single_courses': {
            init: function () {
				 $(document).on('click', '#attend_lecture .wpcf7-submit', function(){
                    if(typeof fbq === "function") {
                        fbq('track', 'Lead', {
                            content_category: 'Course form submission',
                            content_name: 'Attend a lecture'
                        });
                    }
                });
                $(document).on('click', '#start_application .wpcf7-submit', function(){
                    if(typeof fbq === "function") {
                        fbq('track', 'CompleteRegistration', {
                            content_name: 'Course application'
                        });
                    }
                });
				
                // JavaScript to be fired on the course page
                $(".course-nav a").click(function (evn) {
                    evn.preventDefault();
                    var target = $(this).attr('href');
                    var offsetTop = $(target).offset().top;
                    if (target === '#overview') {
                        offsetTop += 370;
                    } else {
                        if ($('.admin-bar').length) {
                            offsetTop -= 32;
                        } else {
                            offsetTop = $(target).offset().top;
                        }
                    }

                    $('html, body').animate({
                        scrollTop: offsetTop
                    }, 1000);

                });

                $(".sidebar_for_course.desktop a.read_more").click(function (evn) {
                    evn.preventDefault();
                    var target = $(this).attr('href');
                    $('html, body').animate({
                        scrollTop: $(target).offset().top - 44
                    }, 1000);

                });

                var $modal = $('#course_location_modal');
                $modal.focus(function () {
                    $(this).parent('p').addClass('active');
                });
                $modal.blur(function () {
                    if ($(this).val() !== "") {
                        $(this).parent('p').addClass('active');
                    }
                    else {
                        $(this).parent('p').removeClass('active');
                    }
                });
                // Init scrollspy for sidebar
                $body.scrollspy({target: '#floating-navbar', offset: 100});

                $(window).on('load scroll', function () {
                    var $sidebar = $('.sidebar_for_course.desktop');
                    var bannerHeight = $('.banner').outerHeight(true) + 12; // the "12" should equal the margin-top value for nav.stick
                    var window_top = $(window).scrollTop() + bannerHeight;
                    var div_top = $('#nav-anchor').offset().top;
                    var offset_bottom = window_top + $sidebar.outerHeight() + 130;
                    var window_bottom = $('.footer-top').offset().top + bannerHeight;
                    var offset = (window_bottom - $sidebar.outerHeight() - 100) - div_top;
                    if (window_top > div_top) {
                        $sidebar.addClass('stick');
                        if (offset_bottom > window_bottom) {
                            $sidebar.css({
                                'position': 'absolute',
                                'top': offset
                            });
                        }
                        else {
                            $sidebar.removeAttr('style');
                        }
                    }
                    else {
                        $sidebar.removeClass('stick');
                    }

                });
            }
        },
        // Event single page
        'single_events': {
            init: function () {
                // JavaScript to be fired on the event page
                var map = new GMaps({
                    div: '#map-event',
                    // disableDefaultUI: true,
                    scrollwheel: false,
                    navigationControl: false,
                    mapTypeControl: false,
                    // scaleControl: false,
                    // draggable: false,
                    //zoomSet: 4,
                    lat: 49.5684946,
                    lng: 34.5832272
                });
                GMaps.geocode({
                    address: $('#map-event').data('address'),
                    callback: function (results, status) {
                        if (status === 'OK') {
                            var latlng = results[0].geometry.location;

                            map.setCenter(latlng.lat(), latlng.lng());
                            map.addMarker({
                                lat: latlng.lat(),
                                lng: latlng.lng(),
                                icon: {
                                    url: markers.event,
                                    scaledSize: new google.maps.Size(65, 65)
                                }
                            });
                        }
                    }
                });

                //Scroll to map on event page
                $("a.view_on_map").click(function () {
                    var elementClick = $(this).attr("href");
                    var destination = $(elementClick).offset().top;
                    $("html:not(:animated), body:not(:animated)").animate({
                        scrollTop: destination
                    }, 800);
                    return false;
                });
            },
            finalize: function () {
                // JavaScript to be fired on the event page, after the init JS
            }
        },
        // Location single page
        'single_locations': {
            init: function () {
                // JavaScript to be fired on the location page
                var map = new GMaps({
                    div: '#gmap_small',
                    // disableDefaultUI: true,
                    scrollwheel: false,
                    navigationControl: false,
                    mapTypeControl: false,
                    // scaleControl: false,
                    // draggable: false,
                    lat: 0,
                    lng: 0
                });
                GMaps.geocode({
                    address: $('#gmap_small').data('address'),
                    callback: function (results, status) {
                        if (status === 'OK') {
                            var latlng = results[0].geometry.location;

                            map.setCenter(latlng.lat(), latlng.lng());
                            map.addMarker({
                                lat: latlng.lat(),
                                lng: latlng.lng(),
                                icon: {
                                    url: markers.location,
                                    scaledSize: new google.maps.Size(65, 65)
                                }
                            });
                        }
                    }
                });
            },
            finalize: function () {
                // JavaScript to be fired on the location page, after the init JS
            }
        },
        // Stories page
        'page_template_stories': {
            init: function () {
                //console.log('test');
                // JavaScript to be fired on the stories page
                // Ajax JS
                var ppp = 3; // Post per page
                var offset = 9; // Offset
                var pageNumber = 1;
                var loading = false;
                var havePosts = true;


                function load_posts() {
                    $(document).ajaxStart(function () {
                        $("#wait").css("display", "block");
                        $(".story").css("opacity", "0.5");
                        $("#more_posts").css({
                            'opacity': 0,
                            'cursor': 'default'
                        });
                    });
                    $(document).ajaxComplete(function () {
                        $(".story").css("opacity", "1");
                        $("#wait").css("display", "none");
                    });
                    pageNumber++;
                    var str = '&offset=' + offset + '&pageNumber=' + pageNumber + '&ppp=' + ppp + '&action=more_post_ajax';
                    if (!loading && havePosts) {
                        loading = true;
                        $.ajax({
                            type: "POST",
                            dataType: "html",
                            url: ajaxurl,
                            data: str,
                            success: function (data) {
                                var $data = $(data);
                                if ($data.length) {
                                    // Add new posts
                                    $('#ajax-posts').append($data);
                                    // MatchHeight to posts
                                    $('.stories_equal_height').matchHeight(
                                        {
                                            byRow: false,
                                        }
                                    );
                                    $('.story h5.height').matchHeight();
                                    $('.story p.height').matchHeight();
                                    // Count posts on page
                                    var postsLength = $('.stories_equal_height').length;
                                    // New offset
                                    offset = postsLength;
                                    $("#more_posts").attr("disabled", false);
                                    $("#more_posts").css("visibility", 'hidden');
                                } else {
                                    $("#more_posts").attr("disabled", true);
                                    havePosts = false;
                                }
                                loading = false;
                            },
                            error: function (jqXHR, textStatus, errorThrown) {
                                $loader.html(jqXHR + " :: " + textStatus + " :: " + errorThrown);
                            }

                        });
                    }

                    return false;
                }

                // Stories lazy loading
                $body.on("click", '#more_posts', function (e) { // When btn is pressed.
                    e.preventDefault();
                    $("#more_posts").attr("disabled", true); // Disable the button, temp.
                    load_posts();
                });
                // Stories category filter
                $('a.category').on("click", function (e) {
                    e.preventDefault();
                    var name_category = $(this).attr('href');
                    $('a').parent('li').removeClass('active');
                    $('a.' + name_category).parent('li').addClass('active');
                    $('div.type-post').css('display', 'none');
                    $('div.type-post.' + name_category).css('display', 'block');
                });
                $('a.category-all').on("click", function (e) {
                    e.preventDefault();
                    $('a').parent('li').removeClass('active');
                    $('a.category-all').parent('li').addClass('active');
                    $('div.type-post').css('display', 'block');
                });


                // Slick carousel
                $('.stories_page .skew .posts').slick({
                    infinite: false,
                    slidesToShow: 1,
                    slidesToScroll: 1,
                    fade: true,
                    prevArrow: $('.prev_story'),
                    nextArrow: $('.next_story')
                });

                $(window).on('scroll', function () {

                    var offsetLastRowPosts = parseInt($('#more_posts').offset().top);
                    var scrollTop = parseInt($(window).scrollTop() + $('.post:last-child').height());
                    if (offset > 9 && scrollTop > offsetLastRowPosts) {
                        load_posts();
                    }
                });
            }
        },
        // About page
        'page_template_about': {
            init: function () {
                // JavaScript to be fired on the about page

            },
            finalize: function () {
                // JavaScript to be fired on the about page, after the init JS
                var checkSVGS = setInterval(function () {
                    var svg = $('.replaced-svg');
                    if (svg.length > 0) {
                        svg.each(function (index, svgobj) {
                            var bbox = svgobj.getBBox();
                            var viewBox = [bbox.x, bbox.y, bbox.width, bbox.height].join(" ");
                            svgobj.setAttribute("viewBox", viewBox);
                        });
                        clearInterval(checkSVGS);
                    }

                }, 200)
            }
        }
    };


    // scolarshp page
    var image_resize = function resize_img() {
        var opt_img = $('.options .option_image');
        var width_opt_img = opt_img.width();
        opt_img.height(width_opt_img);
    }
    image_resize();
    $(window).resize(image_resize);

    var count = 0;
    $(".tuition #firstWord").each(function () {
        count++;
        $(this).html(function () {
            var text = $(this).text().trim().split(" ");
            var first = text.shift();
            return (text.length > 0 ? "<span class='color-" + count + "'>" + first + "</span> " : first) + text.join(" ");
        });
    });

    // The routing fires all common scripts, followed by the page specific scripts.
    // Add additional events for more control over timing e.g. a finalize event
    var UTIL = {
        fire: function (func, funcname, args) {
            var fire;
            var namespace = Sage;
            funcname = (funcname === undefined) ? 'init' : funcname;
            fire = func !== '';
            fire = fire && namespace[func];
            fire = fire && typeof namespace[func][funcname] === 'function';

            if (fire) {
                namespace[func][funcname](args);
            }
        },
        loadEvents: function () {
            // Fire common init JS
            UTIL.fire('common');

            // Fire page-specific init JS, and then finalize JS
            $.each(document.body.className.replace(/-/g, '_').split(/\s+/), function (i, classnm) {

                UTIL.fire(classnm);
                UTIL.fire(classnm, 'finalize');
            });

            // Fire common finalize JS
            UTIL.fire('common', 'finalize');
        }
    };

    // Load Events
    $(document).ready(UTIL.loadEvents);

    // testimonial slider
    var $testimonialsSlider = $('.be-testimonials-slider'),
        $testimonialsPagi = $('.be-testimonial__pagi'),
        $activeClass = 'be-testimonial__pagi--active';

    if ($testimonialsSlider.length) {
        $testimonialsSlider.slick({
            arrows: false,
            infinite: false,
            adaptiveHeight: false,
            fade: true,
            autoplay: true,
            autoplaySpeed: 5000
        });
        $testimonialsPagi.on('click', function (e) {
            e.preventDefault();
            var $index = $(this).data('slide');
            $testimonialsSlider.slick('slickGoTo', $index);
        });

        $testimonialsSlider.on('beforeChange', function (event, slick, currentSlide, nextSlide) {
            $testimonialsPagi.removeClass($activeClass);
            $testimonialsPagi.filter('[data-slide="' + nextSlide + '"]').addClass($activeClass);
        });
    }

    // Animation label
    var $inputFields = $('.be-input__field');

    $inputFields.on('focus', function () {
        $(this).addClass('be-input--filled');
    });
    $inputFields.on('blur', function () {
        if (!$(this).val()) {
            $(this).removeClass('be-input--filled');
        }
    });

    // Fix add span CF7 for field animation
    //$(".wpcf7-form").find(".wpcf7-form-control-wrap:not(.be-input)").contents().unwrap();
