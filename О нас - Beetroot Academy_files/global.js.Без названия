    /* Show schedule description*/
    $('.schedule-info__toggle').on({
        'mouseenter': function (e) {
            var $this = $(this);
            var scheduleDesc = $this.data('id');
            var parentScheduleItems = $this.closest('.schedule-info__item');
            if ($('.' + scheduleDesc).length) {
                var scheduleItems = $(document).find('.schedule-info__item').not($(parentScheduleItems));
                $(document).find('.schedule-info__item-desc').removeClass('is-active');
                $('.' + scheduleDesc).addClass('is-active');
                parentScheduleItems.addClass('is-active');
                $(scheduleItems).each(function () {
                    $(this).addClass('is-opacity');
                });
                $this.css
            }
        },
        'mouseleave': function (e) {
            $(document).find('.schedule-info__item-desc, .schedule-info__item').removeClass('is-active is-opacity');
        },
        'click touchend': function (e) {
            e.preventDefault();
            if (window.matchMedia('(max-width: 1920px)').matches) {
                if (e.type == 'touchend') {
                    $(this).click();
                    var $this = $(this);
                    var scheduleDesc;
                    if (window.matchMedia('(max-width: 1920px)').matches) {
                        scheduleDesc = $this.data('id') + '-mobile';
                    } else {
                        scheduleDesc = $this.data('id');
                    }

                    var parentScheduleItems = $this.closest('.schedule-info__item');
                    if ($('.' + scheduleDesc).length) {
                        if (!parentScheduleItems.hasClass('is-active')) {
                            var scheduleItems = $(document).find('.schedule-info__item').not($(parentScheduleItems));
                            $(document).find('.schedule-info__item-desc, .schedule-info__item , .schedule-info__item-desc').removeClass('is-active is-opacity');
                            $('.' + scheduleDesc).addClass('is-active');
                            parentScheduleItems.addClass('is-active');
                            var scheduleDesctop = $this.data('id');
                            $('.' + scheduleDesctop).addClass('is-active');
                            $(scheduleItems).each(function () {
                                $(this).addClass('is-opacity');
                            });
                        } else {
                            $(document).find('.schedule-info__item-desc, .schedule-info__item').removeClass('is-active is-opacity');
                        }
                    }
                }
            }
        }
    });

    /* Students testimonials slider*/
    // Slick carousel
    $('.testimonials__slider').slick({
