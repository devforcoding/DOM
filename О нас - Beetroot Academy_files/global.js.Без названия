    // popups

    $(document).on('click', '[data-popup]', function (e) {
        e.preventDefault();
        var $this = $(this);
        var dataModal = $this.data('popup');
        var modal = $('#' + dataModal);


        // with FAQ
        if ($this.hasClass('be-process')) {
            $('.be-process').removeClass('be-process--active');
            $this.addClass('be-process--active');
            var $center = $(window).outerHeight() / 2 - 70;
            $('html, body')
                .animate({
                    scrollTop: $this.offset().top - $center
                }, 400);
            setTimeout(function () {
                if (modal.length) {
                    body.toggleClass('be-modal-open');
                    modal.toggleClass('open');
                }
            }, 400);
        } else {
            modal.toggleClass('open');
            body.toggleClass('be-modal-open');
        }
        var dataPopupMsg = $this.data('popup-message')
        if (dataPopupMsg) {
            $('[name="description"]', modal).focus().val(dataPopupMsg).trigger('input');
        }
        if (dataPopupMsg == ' ') {
            $('[name="description"]', modal).focus().val("").trigger('input');
        }

    });

    $(document).on('click', '.be-overlay, .be-popup a.close', function (e) {
        e.preventDefault();
        var modal = $('.be-popup.open');
        modal.removeClass('open');
        body.removeClass('be-modal-open');
    });

    //Show CTA form (career)
    $(document).on('click', '.toggle-form', function () {
        $('#' + $(this).data('toggle')).toggleClass('hide');
    });

    //Sticky custom top bar for Career page
    $(window).scroll(function () {
        if ($(window).scrollTop() > 120) {
            $('.be-page-nav.sticky').addClass('is-anchored is-stuck');
        } else {
            $('.be-page-nav.sticky').removeClass('is-anchored is-stuck');
        }
    });

    //Career page Page Nav
    $pageNavLinks = $('.be-page-nav ul a[href^="#"], .be-scroll');

    $(document).on("scroll", onScroll);

    $pageNavLinks.on('click', function (e) {
        e.preventDefault();
        $(document).off("scroll");

        $pageNavLinks.each(function () {
            $(this).removeClass('be-active');
        });
        $(this).addClass('be-active');
        var target = this.hash,
            menu = target;
        $target = $(target);
        $('html, body').stop().animate({
            'scrollTop': $target.offset().top + 2
        }, 600, 'swing', function () {
            window.location.hash = target;
            $(document).on("scroll", onScroll);
        });
    });

    function onScroll(event) {
        var scrollPos = $(document).scrollTop();
        $pageNavLinks.removeClass("be-active");
        $pageNavLinks.each(function () {
            var currLink = $(this);
            var refElement = $(currLink.attr("href")),
                refElementTop = refElement.offset().top - 75,
                refElementHeight = refElement.outerHeight();
            if (refElementTop <= scrollPos && refElementTop + refElementHeight > scrollPos) {
                currLink.addClass("be-active");
            }
            else {
                currLink.removeClass("be-active");
            }
        });
    }

    // Autoresize texarea
    var $txtAutores = $('.be-autoresize');

    if ($txtAutores.length) {
        var hiddenDiv = $(document.createElement('div')),
            content = null;


        hiddenDiv.addClass('be-autoresize-div').css('width', $txtAutores.actual('width'));

        $body.append(hiddenDiv);

        $txtAutores.on('input propertychange', function () {

            content = $(this).val();

            content = content.replace(/\n/g, '<br>');
            hiddenDiv.html(content + '<br class="lbr">');

            $(this).css('height', hiddenDiv.outerHeight());

        });
    }

    function hideFooterOnTop() {
        if ($(window).width < 768) {
            if ($(window).scrollTop() < 200) $('#footer-container').css({'visibility': 'hidden'});
            else $('#footer-container').css({'visibility': 'visible'});
        }
    }

    hideFooterOnTop();

    $(window).on('scroll resize', function () {
        hideFooterOnTop();
    });

    $('.be-job__location').on('click', function (e) {
        e.preventDefault();
        $('.be-job').focus();
    });

