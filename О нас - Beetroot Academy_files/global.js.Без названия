    /*Elipsis teacher description*/

    ;(function () {
        var teamMemberHeight = $('.teammember-info').find('.teammember-desc__inner').height();
        $(document).on('click', '.teammember-desc__toggle', function (e) {
            e.preventDefault();
            var $this = $(this),
                teamMemberInfo = $this.closest('.teammember-info').find('.teammember-desc__inner'),
                teamMemberInfoHeight = teamMemberInfo.height(),
                thisText = $this.find('span');
            if (teamMemberInfoHeight <= teamMemberHeight) {
                autoHeightAnimate(teamMemberInfo, animateTime);
                thisText.addClass('is-active');
            } else {
                teamMemberInfo.stop().animate({height: teamMemberHeight}, animateTime);
                thisText.removeClass('is-active');
            }
        });
    })();

    /*Toggle student work modal iframe */
    ;(function () {
        $(document).on('click', '.student-portfolio__link', function (e) {
            e.preventDefault();
            e.stopPropagation();
            var regEx = /behance/i;
            if($(this).attr('href').match(regEx)){
                window.open($(this).attr('href'), '_blank');
            }else{
                var $this = $(this),
                    $modal = $('#student_work'),
                    $modalName = $modal.find('.modal-title__content-name'),
                    $modalCourse = $modal.find('.modal-title__content-course'),
                    $modalFrame = $modal.find('.modal-body__frame'),
                    $frameError = $modal.find('.modal-body__frame-link'),
                    $frameText = $modal.find('.modal-body__frame-text'),
                    link = $this.attr('href'),
                    name = $this.find('img').attr('alt'),
                    courseName = $('.students-portfolio__head > p').text();

                /*text.addClass('is-loading');
                text.removeClass('is-loading');*/

                $modalName.html(name);
                $modalCourse.html(courseName);
                $modalFrame.append('<object id="modal-frame" type="text/html" data="' + link + '" style="overflow:auto;"></object>');
                $frameError.attr('href', link);
                $modal.addClass('is-active');
                $body.addClass('is-open');
                $('.banner').css('z-index', '3');
                setTimeout(function () {
                    $frameText.addClass('is-active');
                }, 1000);
                if (iOS()) {
                    $('#modal-frame').addClass('pointer-none');
                }
            }
        });
    })();

    ;(function () {
        $(document).on('click touchend', '.modal-frame__close', function (e) {
            e.preventDefault();
            e.stopPropagation();
            closeModalFrame();
        });

        $(document).on('click touchend', '.modal-body__frame-link', function (e) {
            var $this = $(this);
            if (e.type === 'touchend') {
                $this.click();
            }
            if (iOS()) {
                window.location.href = $this.attr('href');
            }
            setTimeout(function () {
                closeModalFrame();
            }, 100);

        });

        $(document).on('click touchend', '[data-toggle="modal"]', function () {

        });
    })();


    $(window).on('load resize', function () {
        /* Set iframe height depend on header height */
        var $modalIframe = $('.modal-body__frame'),
            $modalHeaderHeight = $('#student_work').find('.modal-header').outerHeight();
        $modalIframe.css('top', '' + $modalHeaderHeight + 'px');

        /*Delete whole classes schedule info*/
        $(document).find('.schedule-info__item-desc, .schedule-info__item').removeClass('is-active is-opacity');

    });

    /*Action on contact form 7 seccess submit*/
    $(document).on('wpcf7mailsent', '.wpcf7', function (event) {
        var $popupForm = $('#success_form');
        var $dataTexts = $(this).parents('[data-form-confirm-title]');
        var popupTitle = $dataTexts.data('form-confirm-title');
        var popupText = $dataTexts.data('form-confirm-text');
        var $parentModal = $(this).parents('.modal.in');

        if (popupTitle) $popupForm.find('.modal-inner-title').text(popupTitle);
        if (popupText) $popupForm.find('.modal-footer__text').text(popupText);
        if ($parentModal.length) {
            $parentModal.find('.close').click();
        }

        $(this).trigger('reset');
        $popupForm.addClass('is-active');
    });

    $(document).on('click', '#success_form .close', function () {
        $('#success_form').removeClass('is-active');
        $(document).find('.modal .close').click();
        wpcf7ClearResponseOutput($('.wpcf7'));

    });

    /* Add adroll code on click event*/
    $(document).on('click', '.content_course_form .submit_form', function () {
        try {
            __adroll.record_user({"adroll_segments": "ccdd8b82"})
        } catch (err) {
        }
    });


    /* Function to animate height: auto */
    function autoHeightAnimate(element, time) {
        var curHeight = element.height(),
            autoHeight = element.css('height', 'auto').height();
        element.height(curHeight);
        element.stop().animate({height: autoHeight}, time);
    }

    /*Reset CF7 form*/
    function wpcf7ClearResponseOutput(form) {
        form.find('.active').removeClass('active');
        form.find('div.wpcf7-response-output').html('').empty().removeClass('wpcf7-mail-sent-ok wpcf7-mail-sent-ng wpcf7-validation-errors wpcf7-spam-blocked');
        $('span.wpcf7-not-valid-tip').remove();
        $('img.ajax-loader').css({visibility: 'hidden'});
    }

    /*Close modal iframe*/
    function closeModalFrame() {
        var $modal = $('#student_work'),
            $frameError = $modal.find('.modal-body__frame-link'),
            $frameText = $modal.find('.modal-body__frame-text'),
            $modalFrame = $modal.find('.modal-body__frame');
        $modal.addClass('is-hide');
        $body.removeClass('is-open');
        $('.banner').attr('style', '');
        setTimeout(function () {
            $modal.removeClass('is-active is-hide');
            $(document).find('.is-active').removeClass('is-active');
            $modalFrame.find('#modal-frame').remove();
            $frameError.attr('href', '#');
            $frameText.removeClass('is-active');
        }, 300);
    }

    /*Detect iOS*/
    function iOS() {
        var iDevices = [
            'iPad Simulator',
            'iPhone Simulator',
            'iPod Simulator',
            'iPad',
            'iPhone',
            'iPod',
            'MacIntel'
        ];
        if (!!navigator.platform) {

            while (iDevices.length) {
                if (navigator.platform === iDevices.pop()) {
                    return true;
                }
            }
        }
        return false;
    }

    /*Python Template*/

    if ($('body').hasClass('python-template')) {

        var keys = {37: 1, 38: 1, 39: 1, 40: 1};

        function preventDefault(e) {
            e = e || window.event;
            if (e.preventDefault)
                e.preventDefault();
            e.returnValue = false;
        }

        function preventDefaultForScrollKeys(e) {
            if (keys[e.keyCode]) {
                preventDefault(e);
                return false;
            }
        }

        function disableScroll() {
            if (window.addEventListener) // older FF
                window.addEventListener('DOMMouseScroll', preventDefault, false);
            window.onwheel = preventDefault; // modern standard
            window.onmousewheel = document.onmousewheel = preventDefault; // older browsers, IE
            window.ontouchmove = preventDefault; // mobile
            document.onkeydown = preventDefaultForScrollKeys;
        }

        function enableScroll() {
            if (window.removeEventListener)
                window.removeEventListener('DOMMouseScroll', preventDefault, false);
            window.onmousewheel = document.onmousewheel = null;
            window.onwheel = null;
            window.ontouchmove = null;
            document.onkeydown = null;
        }

        function stopScroll(e) {
            if (e.originalEvent.deltaY < 0 && $(this).scrollTop() == 0) return false;

            if ($(this).scrollTop() > 0) {
                $(window).off('wheel', stopScroll);
            } else {
                disableScroll();
                $(this).scrollTop(100);
                setTimeout(function () {
                    enableScroll();
                    $(window).off('wheel', stopScroll);
                }, 500);

                e.preventDefault();
            }
        }

        $(window).on('wheel', stopScroll);

        $(window).scroll(function () {

            // if ($(this).scrollTop() == 0) {
            //     $(window).on('wheel', stopScroll);
            // }

            if ($(this).scrollTop() > 0) {
                $('header.python-template-header').addClass("sticky");
                $('body.python-template').addClass("sticky");
                $('.head_course').addClass("sticky");
                $('.head_course-fadeout').addClass("fadeout");
                $('.head_course-fadein').addClass("fadeout");
                $('.image_title').addClass('resize');
            }
            // else {
            //     $('header.python-template-header').removeClass("sticky");
            //     $('body.python-template').removeClass("sticky");
            //     $('.head_course').removeClass("sticky");
            //     $('.head_course-fadeout').removeClass("fadeout");
            //     $('.head_course-fadein').removeClass("fadeout");
            //     $('.image_title').removeClass('resize');
            //
            // }
        });


        $('[data-steps]').click(function () {
            $('.steps_wrapper').addClass('show');
            $('[data-steps]').hide();
            $('.active-steps').removeClass('active-steps');

        });

        $("#primary-menu").on("click", "a", function (event) {

            event.preventDefault();

            var id = $(this).attr('href'),

                top = $(id).offset().top;

            $('body,html').animate({scrollTop: top - 180}, 1000);
        });

        $("[data-smooth]").on("click", function (event) {

            event.preventDefault();

            var id = $(this).attr('href'),

                top = $(id).offset().top;

            $('body,html').animate({scrollTop: top - 180}, 1000);
        });

        $('.menu-python-menu-container ul li a').on('click', function () {
            $('.burger_menu').parent().toggleClass('active');
            $('.burger_menu').removeClass('active');
            $('.burger_menu a').removeClass('active');
            $('.mobile_nav').removeClass('active');
        });
    }
    $(document).ready(function () {
        var $teamMember = $('.teammember-card');

        if ($teamMember.length > 0) {
            $teamMember.each(function (i) {
                var memberDescription = $(this).find('.teammember-desc__inner');
                if (memberDescription.text() === '' || memberDescription.text().length < 100) {
                    $(this).find('.teammember-desc__toggle-top').css('display', 'none')
                }
            });
        }
    });


})(jQuery); // Fully reference jQuery after this point.

